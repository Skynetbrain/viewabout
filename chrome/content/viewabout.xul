            <menuitem value="about:downloads" label="&aboutDownloads.label;" accesskey="&aboutDownloads.accesskey;"/>
            <!-- about:downthemall -->
            <menuitem value="about:downthemall" label="&aboutDownthemall.label;" accesskey="&aboutDownthemall.accesskey;"/>
